{
  "manifest_version": 3,
  "name": "ZenSurf",
  "version": "1.0.0",
  "author": "The Struggle Bus",
  "description": "Provide various exercises to reduce anxiety without changing tab",
  "icons": {
    "16": "images/extIcon.png",
    "32": "images/extIcon.png",
    "48": "images/extIcon.png",
    "128": "images/extIcon.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "ZenSurf",
    "default_icon": "images/extIcon.png"
  },
  "permissions": ["tabs"],
  "content_scripts": [
    {
      "js": ["popup.js"],
      "matches": [
        "https://www.google.com/*",
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome/docs/webstore/*"
      ]
    }
  ]
}
