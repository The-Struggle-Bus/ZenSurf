{
    "manifest_version": 3,
    "name": "ZenSurf",
    "version": "1.0.0",
    "author": "The Struggle Bus",
    "description": "Provide various exercises to reduce anxiety without changing tab",
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_title": "ZenSurf",
        "default_icon": "images/icon.png"
    },
    "permissions": [
        "tabs"
    ]
